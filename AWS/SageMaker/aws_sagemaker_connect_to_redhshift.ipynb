   "source": [
    "def execute_query(query: str) -> pd.core.frame.DataFrame:\n",
    "    cursor = conn.cursor()\n",
    "    cursor.execute(query)\n",
    "    rows = cursor.fetchall()\n",
    "    complete_df = pd.DataFrame(rows, columns = list(zip(*cursor.description))[0])\n",
    "    cursor.close()\n",
    "    return complete_df"
   ]
  },
